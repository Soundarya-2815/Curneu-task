  .momentum__headline p span {
     font-size: 2.75rem !important;
  }
}

@media (min-width: 1920px) {
  .momentum__video-title p span {
     font-size: 1.375rem !important;
  }
  .momentum__headline p span {
     font-size: 3.375rem !important;
  }
